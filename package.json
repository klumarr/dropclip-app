        "eslint": "^8.57.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.16",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "ts-node": "^10.9.2",
        "typescript": "5.3.3",
