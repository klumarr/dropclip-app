        "qrcode.react": "^4.2.0",
        "react": "^18.2.0",
        "react-beautiful-dnd": "^13.1.1",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.21.1",
        "recharts": "^2.10.3",
        "stream-browserify": "^3.0.0",
        "tesseract.js": "^4.1.1",
        "util": "^0.12.5"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.133",
        "@types/dotenv": "^6.1.1",
        "@types/node": "^20.17.12",
        "@types/qrcode.react": "^1.0.5",
        "@types/react": "^18.2.47",
